{
  "manifest_version": 2,

  "name": "Google Map - geolocation",
  "description": "This extension adds an option of auto-filling geolocation for \"Get directions\" in google maps",
  "version": "1.0",
  "permissions": [
    "contextMenus", "tabs", "https://www.google.com/maps/*"
  ],
  
  
  "background": {
    "scripts": ["scripts/background.js"]
  },
  "icons": { 
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png" }
}
